<svg xmlns="http://www.w3.org/2000/svg" width="512" height="512" viewBox="0 0 512 512">
  <!-- Background with gradient -->
  <defs>
    <linearGradient id="bgGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#6366f1;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8b5cf6;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="serverGrad" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:0.3" />
      <stop offset="100%" style="stop-color:#ffffff;stop-opacity:0.1" />
    </linearGradient>
  </defs>
  
  <!-- Background rounded rectangle -->
  <rect width="512" height="512" fill="url(#bgGrad)" rx="96"/>
  
  <!-- Server icon group -->
  <g transform="translate(106, 120)">
    <!-- Server 1 (Top) -->
    <rect x="0" y="0" width="300" height="75" fill="url(#serverGrad)" stroke="#ffffff" stroke-width="5" rx="12"/>
    <circle cx="40" cy="37.5" r="10" fill="#22c55e"/>
    <circle cx="80" cy="37.5" r="10" fill="#22c55e"/>
    <rect x="120" y="25" width="150" height="25" fill="#ffffff" opacity="0.2" rx="4"/>
    
    <!-- Server 2 (Middle) -->
    <rect x="0" y="95" width="300" height="75" fill="url(#serverGrad)" stroke="#ffffff" stroke-width="5" rx="12"/>
    <circle cx="40" cy="132.5" r="10" fill="#22c55e"/>
    <circle cx="80" cy="132.5" r="10" fill="#eab308"/>
    <rect x="120" y="120" width="150" height="25" fill="#ffffff" opacity="0.2" rx="4"/>
    
    <!-- Server 3 (Bottom) -->
    <rect x="0" y="190" width="300" height="75" fill="url(#serverGrad)" stroke="#ffffff" stroke-width="5" rx="12"/>
    <circle cx="40" cy="227.5" r="10" fill="#22c55e"/>
    <circle cx="80" cy="227.5" r="10" fill="#22c55e"/>
    <rect x="120" y="215" width="150" height="25" fill="#ffffff" opacity="0.2" rx="4"/>
  </g>
  
  <!-- Text -->
  <text x="256" y="395" font-family="Arial, sans-serif" font-size="56" font-weight="bold" fill="#ffffff" text-anchor="middle">DEWATA</text>
  <text x="256" y="450" font-family="Arial, sans-serif" font-size="40" fill="#ffffff" text-anchor="middle" opacity="0.9">RP</text>
</svg>
